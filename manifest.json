{
	"manifest_version": 2,
	"name": "search_wayback_machine",
	"version": "0.3",
	"description": "Search Archive.org Wayback Machine",

	"icons": {
		"48": "icons/icon.png",
		"96": "icons/icon@2x.png"
	},

	"chrome_settings_overrides": {
	  "search_provider": {
		"name": "Archive.org Wayback Machine",
		"search_url": "https://web.archive.org/web/*/{searchTerms}",
		"keyword": "wb",
		"favicon_url": "https://web.archive.org/_static/images/archive.ico"
	  }
	}

}
